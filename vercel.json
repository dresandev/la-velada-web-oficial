{
  "redirects": [
    {
      "source": "/2024/",
      "destination": "/2024",
      "permanent": true
    },
    {
      "source": "/2024/:path*/",
      "destination": "/2024/:path*",
      "permanent": true
    },
    {
      "source": "/:path*/",
      "destination": "/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/2024",
      "destination": "https://2024.infolavelada.com"
    },
    {
      "source": "/2024/:path*",
      "destination": "https://2024.infolavelada.com/:path*"
    }
  ]
}
